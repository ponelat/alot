{
  "name": "alotofdotcoms",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "mocha --compilers js:babel-core/register --recursive --require test/helpers/index.js",
    "test:watch": "npm run test -- --watch",
    "hot-server": "webpack-dev-server --config webpack.hot.config.js --inline --hot --progress",
    "build-deps": "npm run clean && npm run copy-imgs",
    "copy-imgs": "rm -Rf dist/img && mkdir -p dist/img; cp -R src/img/* dist/img/",
    "clean": "rm -rf dist",
    "start-dev": "nodemon -w server/ -w config.js index.js ",
    "dev": "npm run start-dev & npm run hot-server && fg"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "optionalDependencies": {
    "nodemon": "^1.8.1",
    "webpack-dev-server": "^1.12.1"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.4.0",
    "deep-extend": "^0.4.0",
    "expect": "^1.13.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "react": "^0.14.1",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "redux": "^3.0.4",
    "request": "^2.65.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "x-ray": "^2.0.2"
  },
  "dependencies": {
    "babel-core": "^6.3.26",
    "babel-polyfill": "^6.3.14",
    "ejs": "^2.3.4",
    "isomorphic-fetch": "^2.2.0",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.0",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3"
  }
}
